{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowToken",
      "Effect": "Allow",
      "Action": "ecr:GetAuthorizationToken",
      "Resource": "*"
    },
    {
      "Sid": "AllowPull",
      "Effect": "Allow",
      "Action": [
        "ecr:BatchCheckLayerAvailability",
        "ecr:BatchGetImage",
        "ecr:GetDownloadUrlForLayer"
      ],
      "Resource": "arn:aws:ecr:ap-northeast-1:287529267909:repository/myapp"
    },
    {
      "Sid": "AllowGetObjectOnReceipts",
      "Effect": "Allow",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::ai-receipt-apex/uploads/*"
    },
    {
    "Sid": "AllowListReceiptsBucket",
    "Effect": "Allow",
    "Action": "s3:ListBucket",
    "Resource": "arn:aws:s3:::ai-receipt-apex"
    }
  ]
}